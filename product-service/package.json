{
  "name": "@aws-shop-react-backend/product-service",
  "version": "1.0.0",
  "scripts": {
    "validate-swagger": "swagger-cli validate swagger.yaml",
    "predeploy": "npm run validate-swagger && npm run build",
    "build": "webpack",
    "test": "jest",
    "deploy": "cdk deploy",
    "cdk": "cdk"
  },
  "dependencies": {
    "aws-cdk-lib": "^2.0.0",
    "constructs": "^10.0.0"
  },
  "devDependencies": {
    "@apidevtools/swagger-cli": "^4.0.4",
    "@types/aws-lambda": "^8.10.92",
    "@types/jest": "^27.4.0",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^17.0.13",
    "aws-cdk": "^2.0.0",
    "aws-cdk-lib": "^2.0.0",
    "constructs": "^10.0.0",
    "jest": "^27.4.7",
    "js-yaml": "^4.1.0",
    "ts-jest": "^27.1.3",
    "ts-loader": "^9.2.6",
    "typescript": "^4.5.5",
    "webpack": "^5.68.0",
    "webpack-cli": "^4.9.2"
  },
  "jest": {
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/tests"
    ],
    "testMatch": [
      "**/*.test.ts"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    }
  }
}
